<%=form_for @post do |f|%>
<%=f.label :title %>
<%=f.text_field :title %>
<div>
<%=f.label :media_link, "Link"%>
<%=f.text_field :media_link %>
</div>
<div>
<%=f.label :description %>
<%=f.text_area :description %>
</div>
<%=f.label :media_type, "Type of Content"%>
<%=f.select :media_type, ['Article', 'Podcast', 'Video', 'Book']%>
<div>
<%=f.submit%>
<%end%>
</div>
<br>
<%=button_to "Return Home", feed_path, method: "get"%>